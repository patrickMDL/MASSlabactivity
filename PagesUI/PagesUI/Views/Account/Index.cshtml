    <script>
        
        var formLogin = $('#form-login');

        $('#sendingEmail').hide();
        $('#Email').focus();

        $("#PasswordBtnEye").hover(function (e) {
            e.preventDefault();
            if ($("#Password").attr("type") == "password") {
                $("#Password").attr("type", "text");
                $("#PasswordBtnEye").attr("class", "fa fa-eye show_password");
            } else {
                $("#Password").attr("type", "password");
                $("#PasswordBtnEye").attr("class", "fa fa-eye-slash show_password");
            }
        });

        formLogin.submit(function (event) {
            cleanMsg();

            var email = $.trim($('#Email').val()),
                password = $.trim($('#Password').val());

            if (email.length === 0) {
                $('#msg-return').html('Informe o email.');
                return false;
            }
            else if (password.length === 0) {
                $('#msg-return').html('Informe a senha.');
                return false;
            }
            else {
                event.preventDefault();

                if ($("[name='ckbLembrarEmail']").is(":checked"))
                    document.cookie = "Email=" + $('#Email').val();

                $.post('/Account/Login/', formLogin.serialize(),
                    function (data) {
                        if (data  != null) {
                            document.location.href = "Account/Weather";
                        }
                        else {
                            $('#msg-return').html('Não foi possível realizar o seu login, usuário ou senha inválida.');
                        }
                    });
            }
        });

        $(document).ready(function () {
            if (document.cookie) {
                var data = decodeURIComponent(document.cookie).split(';');

                for (var i = 0; i < data.length; i++) {
                    var item = data[i].trim();

                    if (item.indexOf('Email') == 0) {
                        $('#Email').val(item.substring(6, item.length));
                        $("[name='ckbLembrarEmail']").prop('checked', true);
                    }
                }
            }
        });

        function reconciling() {
            cleanMsg();

            var email = $.trim($('#Email').val());

            if (email.length === 0) {
                $('#msg-return').html('Informe o email.');
                return false;
            }
            else {
                $('#modalRecoveryPassword').modal('toggle');
                $("#EmailModal").val($("#Email").val());
            }            
        }

        function cleanMsg() {
            $('#msg-return').text("");
            $('#msg-return-success').text("");
            $('#msg-return-modal').text("");
        }

        function closeModal() {
            $('#modalRecoveryPassword').modal('toggle');
        }

        function sendRecovery() {
            cleanMsg();
            var email = $.trim($('#EmailModal').val());

            if (email.length === 0) {
                $('#msg-return-modal').html('Informe o email.');
                return false;
            }
            else {
                $('#sendingEmail').show();

                $.post('/Account/RecoveryPassword/', { pEmail: email },
                    function (data) {

                        $('#sendingEmail').hide();

                        if (data.ok) {
                            $('#msg-return-success').html('Email enviado.');
                            closeModal();
                        }
                        else {                            
                            $('#msg-return-modal').html('Não foi possível redefinir a sua senha, email inválido.');
                        }
                    });
            }
        }
    
    </script>
